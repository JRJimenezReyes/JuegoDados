          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_4VAD0J8zEe6a8eZwrQ2Tww" name="comprobarJugadores" specification="_4U_cwJ8zEe6a8eZwrQ2Tww">
